def dice(n): 
    sumDice = 0 
    for n in range(n): 
        sumDice += random.randint(1,6) 
    print ('Roll was', sumDice)

def matches(ticket, winner):
    matching = 0
    for number in ticket:
        if number in winner:
            matching += 1
    print (matching)

def rollonehundred(): 
    a = [] 
    for number in range(0, 100): 
        a+= [random.randint(1,6)] 
    plt.hist(a) 
    plt.show()

 